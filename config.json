    "sentinel": {
        "settlementWindow": 0.5,
        "reconnectDelay": 3,
        "heartbeatInterval": 2,
        "maxVetoCount": 3
    },
    "janitor": {
        "explorationDelayMs": 300,
        "remediationDelayMs": 1000,
        "selfHealRoiSeconds": 120
    },
    "vision": {
        "model": "moondream",
        "timeout": 25,
        "ollamaUrl": "http://localhost:11434/api/generate"
    },
    "pii": {
        "mode": "alert",
        "patterns": {}
    },
    "network": {
        "chaos": {
            "enabled": false,
            "latencyMs": 0,
            "blockPatterns": []
        }
    },
    "webhooks": {
        "enabled": false,
        "urls": [],
        "notifyOn": [
            "failure",
            "success"
        ]
